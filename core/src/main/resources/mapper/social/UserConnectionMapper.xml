<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tianma315.core.social.dao.UserConnectionMapper">

    <select id="selectById" resultType="com.tianma315.core.social.domain.UserConnection">
